<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="248.0" prefWidth="254.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="trucks.truckers4.Controllers.LoginView">
   <children>
      <Label alignment="CENTER" layoutX="10.0" layoutY="14.0" text="Truckers" textAlignment="CENTER" underline="true" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0">
         <font>
            <Font name="Cooper Black" size="31.0" />
         </font>
      </Label>
      <TextField fx:id="usernameField" layoutX="53.0" layoutY="67.0" promptText="Username...">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passwordField" layoutX="53.0" layoutY="112.0" promptText="Password...">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </PasswordField>
      <Button layoutX="102.0" layoutY="145.0" mnemonicParsing="false" onAction="#validate" text="Log In" />
      <Button layoutX="78.0" layoutY="186.0" mnemonicParsing="false" onAction="#openRegistration" text="Create Account" />
   </children>
</AnchorPane>
